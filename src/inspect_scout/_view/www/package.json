{
  "name": "@meridianlabs/inspect-scout-viewer",
  "version": "0.0.1",
  "description": "Inspect Scout viewer for evaluation logs.",
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@10.20.0",
  "scripts": {
    "build": "vite build",
    "watch": "vite build --watch",
    "watch:dev": "NODE_ENV=development vite build --watch --mode development",
    "dev": "vite",
    "preview": "vite preview",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "typecheck": "tsc --noEmit",
    "check": "pnpm lint:fix && pnpm format && pnpm typecheck"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/bootstrap": "^5.2.10",
    "@types/css-modules": "^1.0.5",
    "@types/eslint__js": "^9.14.0",
    "@types/json5": "^2.2.0",
    "@types/markdown-it": "^14.1.2",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "postcss-url": "^10.1.3",
    "prettier": "^3.6.2",
    "typed-css-modules": "^0.9.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.46.2",
    "typescript-plugin-css-modules": "^5.2.0",
    "vite": "^7.1.5"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "ag-grid-community": "^34.3.0",
    "ag-grid-react": "^34.3.0",
    "ansi-output": "^0.0.9",
    "arquero": "^8.0.3",
    "asciinema-player": "^3.12.1",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.12.1",
    "clsx": "^2.1.1",
    "immer": "^10.1.3",
    "json5": "^2.2.3",
    "jsondiffpatch": "^0.7.3",
    "markdown-it": "^14.1.0",
    "markdown-it-mathjax3": "^5.2.0",
    "mathjax-full": "^3.2.2",
    "prismjs": "^1.30.0",
    "react": "^19.2.0",
    "react-dom": "^19.1.1",
    "react-popper": "^2.3.0",
    "react-router-dom": "^7.9.4",
    "react-virtuoso": "^4.14.1",
    "zustand": "^5.0.8"
